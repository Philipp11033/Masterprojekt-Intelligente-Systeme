<launch>
    <arg name="num_agents" default="10"/>
    <arg name="robot_goal" default="5.5 0"/>
    <node pkg="simple_dwa_lidar" type="main_node.py" name="simple_dwa_lidar" output="screen">
        <param name="num_agents" value="$(arg num_agents)" />
        <param name="robot_goal" value="$(arg robot_goal)" />
    </node>
</launch>
